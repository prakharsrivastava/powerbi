

  val g = d.map { demoJson =>
    val attribute = demoJson.hcursor.get[Option[String]]("a").getOrElse(None)
    val values = demoJson.hcursor.get[Option[Seq[String]]]("v").getOrElse(None)
    sdp(attribute, values)
  }

  ReportParam(Some(demo))
}

object Main extends App {
  val jsonString = """
  [
    { "a": "g", "values": ["yes", "no"] },
    { "c": "d", "values": ["XL", "M", "S"] }
  ]

}
